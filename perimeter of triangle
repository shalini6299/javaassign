import java.util.*;

class Main 
{
    // Driver method
    public static void main(String[] args)
    {
        //length of 3 sides of triangle declared
        double side1 = 2.0; 
        double side2 = 3.0; 
        double side3 = 5.0;
        // finding the perimeter
        double perimeter=(side1+side2+side3);
        System.out.println("Perimeter of triangle = "+perimeter);
    }
}
