BOXING

class BoxingExample1{  
  public static void main(String args[]){  
    int a=50;  
        Integer a2=new Integer(a);//Boxing  
  
        Integer a3=5;//Boxing  
          
        System.out.println(a2+" "+a3);  
 }   
}  
 
 UNBOXING
 class UnboxingExample1{  
  public static void main(String args[]){  
    Integer i=new Integer(50);  
        int a=i;  
          
        System.out.println(a);  
 }   
}  
